

// javaScript code :


function numberIndigit(){
    let n = 123456;
    let b = 2; // in which form we want numbers in binary or decimal
    
    return Math.round(Math.log(n) / Math.log(b))+1;
}

console.log(numberIndigit());

// Java code : 


class HelloWorld {
    public static void main(String[] args) {
        countNumbers();
    }
    
    static void countNumbers(){
        int n = 1234324;
        int b = 10;
        
        System.out.println(Math.round(Math.log(n) / Math.log(b)) + 1);
    }
}