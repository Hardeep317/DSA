

// JavaScript Code :

function bubbleSort(nums){

    for(let i = 0; i < nums.length; i++){
        for(let j = 1; j < nums.length-1; j++){
            if(nums[j] < nums[j-1]){
                let temp = nums[j];
                nums[j] = nums[j-1];
                nums[j-1] = temp;
            }
        }
    }

    return nums;
}

let arr = [6,2,-1,-55,-3,4];
console.log(bubbleSort(arr));